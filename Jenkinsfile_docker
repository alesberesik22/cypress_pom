pipeline {
    agent any
    stages {
        stage('build') {
                if (env.BRANCH_NAME == '1.0.2') {
                  sh 'git branch -a'
                } else {
                  echo 'Som inde'
                }
        }
        stage('test') {
            steps {
                echo 'testing Dev...'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying Dev...'
            }
        }
    }
}  