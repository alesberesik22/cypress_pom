pipeline {
    agent any

    node {    
      stage('Clone repository') {               
             
            checkout scm    
      }     
      stage('Build image') {         
       
            sh 'docker build -t alesberesik/jenkins_image'   
       }     
      stage('Test image') {           
            sh 'echo test'  
        }     
       stage('Push image') {
           sh 'echo test'  
        }
}
